{
  "name": "Humio",
  "id": "humio-humio2grafana-datasource",
  "type": "datasource",
  "metrics": true,
  "annotations": false,
  "info": {
    "description": "Humio datasouce for grafana",
    "author": {
      "name": "Humio apps",
      "url": "https://humio.com"
    },
    "keywords": ["humio", "datasouce"],
    "logos": {
      "small": "img/humio_logo.svg",
      "large": "img/humio_logo.svg"
    },
    "links": [
      {"name": "GitHub", "url": "https://github.com/humio/humio2grafana"},
      {"name": "Apache License", "url": "https://github.com/humio/humio2grafana/blob/master/LICENSE.md"}
    ],
    "version": "1.0.4",
    "updated": "2018-02-07"
  },
  "routes": [
    {
      "path": "humio-authenticated",
      "method": "GET",
      "url": "http://test-ds",
      "headers": [
        {"name": "Authorization", "content": " Bearer {{.SecureJsonData.humioToken}}"}
      ]
    },
    {
      "path": "humio-authenticated",
      "method": "POST",
      "url": "http://test-ds",
      "headers": [
        {"name": "Authorization", "content": " Bearer {{.SecureJsonData.humioToken}}"}
      ]
    }
  ],
  "dependencies": {
    "grafanaVersion": "5.x.x",
    "plugins": [ ]
  }
}
